{
  "name": "blog",
  "version": "1.0.0",
  "private": true,
  "description": "Jerry K Jia's Blog",
  "author": "Jkker <j@erry.dev>",
  "repository": "Jkker/blog",
  "license": "MIT",
  "engines": {
    "node": ">=14.17"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "vercel deploy",
    "deps": "run-s deps:*",
    "deps:update": "[ -z $GITHUB_ACTIONS ] && yarn add notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:update on CI'",
    "deps:link": "[ -z $GITHUB_ACTIONS ] && yarn link notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:link on CI'",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "test": "run-p test:*",
    "test:lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check"
  },
  "dependencies": {
    "@fisch0920/use-dark-mode": "^2.4.0",
    "@fontsource/jetbrains-mono": "^4.5.10",
    "@fontsource/lexend": "^4.5.12",
    "@headlessui/react": "^1.7.0",
    "@keyvhq/core": "^1.6.14",
    "@keyvhq/redis": "^1.6.21",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.5",
    "expiry-map": "^2.0.0",
    "file-saver": "^2.0.5",
    "giscus": "^1.2.0",
    "got": "^12.4.1",
    "isomorphic-unfetch": "^3.1.0",
    "jszip": "^3.10.1",
    "lodash.throttle": "^4.1.1",
    "lqip-modern": "^1.2.0",
    "next": "^12.2.5",
    "next-api-og-image": "^4.2.1",
    "notion-client": "^6.13.8",
    "notion-types": "^6.13.4",
    "notion-utils": "^6.13.4",
    "p-map": "^5.5.0",
    "p-memoize": "^7.1.0",
    "posthog-js": "^1.29.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-geolocated": "^4.0.3",
    "react-icons": "^4.4.0",
    "react-notion-x": "^6.13.9",
    "react-tweet-embed": "^2.0.0",
    "rss": "^1.2.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.2.5",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.31",
    "@types/responselike": "1.0.0",
    "autoprefixer": "^10.4.9",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-import": "^15.0.0",
    "prettier": "^2.7.1",
    "stylelint": "^14.11.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-config-tailwindcss": "^0.0.6",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.2"
  },
  "resolutions": {
    "next-api-og-image/chrome-aws-lambda": "6.0.0",
    "next-api-og-image/puppeteer-core": "6.0.0"
  },
  "overrides": {
    "next-api-og-image": {
      "chrome-aws-lambda": "6.0.0",
      "puppeteer-core": "6.0.0"
    },
    "cacheable-request": {
      "keyv": "npm:@keyvhq/core@~1.6.6"
    }
  }
}
